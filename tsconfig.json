{
  "include": ["./*.d.ts"],
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve"
  }
}
